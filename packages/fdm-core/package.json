{
  "name": "fdm-core",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "main": "./dist/fdm-core.umd.cjs",
  "module": "./dist/fdm-core.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/fdm-core.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/fdm-core.umd.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "db:generate": "npx drizzle-kit generate",
    "document": "npx typedoc src --plugin typedoc-plugin-missing-exports",
    "build": "vite build",
    "check-types": "tsc --noEmit",
    "preinstall": "npx only-allow pnpm",
    "prepare": "pnpm build",
    "preview": "vite preview",
    "test": "vitest run",
    "watch": "vitest",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.7.2",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.26.2",
    "eslint": "^9.9.0",
    "fs-extra": "^11.2.0",
    "globals": "^15.9.0",
    "typedoc": "^0.26.5",
    "typedoc-plugin-missing-exports": "^3.0.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.1.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.0.2",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@electric-sql/pglite": "^0.2.11",
    "drizzle-graphql": "^0.8.5",
    "drizzle-orm": "^0.35.1",
    "graphql": "^16.9.0",
    "nanoid": "^5.0.7",
    "postgres": "^3.4.4"
  },
  "packageManager": "pnpm@9.12.2"
}

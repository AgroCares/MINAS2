        },
        "fields": {
            "description": "Properties of fields",
            "$ref": "https://github.com/AgroCares/farm-data-model/blob/main/schema/fdm_fields.json"
        },
        "aquiring": {
            "description": "Properties of a aquiring fields",
            "$ref": "https://github.com/AgroCares/farm-data-model/blob/main/schema/fdm_aquiring_fields.json"

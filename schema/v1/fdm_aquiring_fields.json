{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://github.com/AgroCares/farm-data-model/blob/main/schema/v1/fdm_aquiring_fields.json",
    "title": "Aquire event of field for a farm in Farm Data Model",
    "description": "The data model to describe the aquire of field event and connect farm and field",
    "type": "object",
    "properties": {
        "b_id": {
            "description": "ID of the field",
            "type": "string"
        },
        "b_id_farm": {
            "description": "ID of the farm",
            "type": "string"
        },
        "b_aquire_start": {
            "description": "Date on which field is aquired for farm",
            "type": "string",
            "pattern":" ^\d{4}\-(0[1-9]|1[012])\-(0[1-9]|[12][0-9]|3[01])$"
        },
        "b_aquire_end": {
            "description": "Date on which field is dispersed from farm",
            "type": "string",
            "pattern": "^\d{4}\-(0[1-9]|1[012])\-(0[1-9]|[12][0-9]|3[01])$"
        },
        "b_aquire_type": {
            "description": "Type of aquirement of field",
            "type": "string",
            "enum": ["ownership", "lease"]
        }
    },
    "required": ["b_id", "b_id_farm"]
}
{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://github.com/AgroCares/farm-data-model/blob/main/schema/v1/fdm_harvesting.json",
    "title": "harvesting event in Farm Data Model",
    "description": "The data model to describe harvesting event of a cultivation",
    "type": "object",
    "properties": {
        "b_id_harvestable": {
            "description": "ID of harvestable",
            "type": "string"
        },
        "b_lu_yield": {
            "description": "Amount of yield",
            "type": "number"
        }
    },
    "required": ["b_id_harvestable"]
}